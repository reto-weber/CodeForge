{
    "compilers": {
        "python": {
            "compile_cmd": "",
            "run_cmd": "python.exe {file}"
        },
        "c": {
            "compile_cmd": "gcc {file} -o {output} -Wall",
            "run_cmd": "{output}"
        },
        "cpp": {
            "compile_cmd": "g++ {file} -o {output} -Wall -std=c++14",
            "run_cmd": "{output}"
        },
        "java": {
            "compile_cmd": "javac {file}",
            "run_cmd": "java -cp {file%/*} {classname}"
        }
    },
    "default_language": "python"
}